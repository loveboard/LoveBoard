            view-source:https://gridstackjs.com/demo/serialization.html#

                function saveGrid() {
                    delete serializedFull;
                    serializedData = grid.save();
                    document.querySelector('#saved-data').value = JSON.stringify(serializedData, null, '  ');
                }


